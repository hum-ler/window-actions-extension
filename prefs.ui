<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <object class="GtkGrid" id="prefsWidget">
    <property name="column-spacing">12</property>
    <property name="row-spacing">12</property>
    <style>
      <class name="grid" />
    </style>
    <child>
      <object class="GtkLabel">
        <property name="label">Show button for Minimize</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showMinimizeButtonSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show toggle for Maximize</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showMaximizeToggleSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">1</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show button for Close</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showCloseButtonSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show button for Shade</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showShadeButtonSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">3</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show button for Move to Workspace Left</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showMoveToWorkspaceLeftButtonSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">4</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show button for Move to Workspace Right</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showMoveToWorkspaceRightButtonSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">5</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show toggle for Always on Top</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showAlwaysOnTopToggleSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">6</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Show toggle for Always on Visible Workspace</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="showAlwaysOnVisibleWorkspaceToggleSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">7</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <layout>
          <property name="column">0</property>
          <property name="row">8</property>
          <property name="column-span">2</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkLabel">
        <property name="label">Monitor the current focus window for changes (requires restart)</property>
        <property name="halign">GTK_ALIGN_START</property>
        <layout>
          <property name="column">0</property>
          <property name="row">9</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="GtkSwitch" id="monitorCurrentFocusWindowSwitch">
        <layout>
          <property name="column">1</property>
          <property name="row">9</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
